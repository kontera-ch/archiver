{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config ./jest-test.json",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config ./jest-test.json",
    "test:nyc": "nyc",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest.js --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --runInBand --config ./jest-e2e.json",
    "test:e2e-debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest.js --runInBand --config ./jest-e2e.json"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.10.1",
    "@google-cloud/storage": "^5.16.1",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.1.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/schedule": "^1.0.2",
    "@sentry/integrations": "^6.16.1",
    "@sentry/node": "^6.16.1",
    "@taquito/local-forging": "^11.2.0",
    "@taquito/signer": "^11.1.0",
    "@taquito/taquito": "^11.1.0",
    "@tzstamp/helpers": "^0.3.4",
    "@tzstamp/proof": "^0.3.4",
    "@tzstamp/tezos-merkle": "^0.3.4",
    "airgap-coin-lib": "^0.9.14",
    "base58check": "^2.0.0",
    "blakejs": "^1.1.1",
    "bs58check": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "merkletreejs": "^0.2.27",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "jest": "^27.3.0",
    "jest-junit": "^13.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
